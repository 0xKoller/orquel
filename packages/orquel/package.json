{
  "name": "orquel",
  "version": "0.1.0",
  "description": "Meta package for Orquel RAG toolkit with CLI wizard",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "orquel": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest",
    "postinstall": "node dist/postinstall.js"
  },
  "keywords": [
    "orquel",
    "rag",
    "retrieval",
    "embeddings",
    "cli"
  ],
  "author": "0xKoller",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/inquirer": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@orquel/core": "workspace:*",
    "commander": "^12.1.0",
    "inquirer": "^12.0.0"
  }
}